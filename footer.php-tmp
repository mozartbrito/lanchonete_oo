        function calculaCompra(valor) {
            let valor_range = valor;
            let valor_unidade = $('#valor_unidade').val();
            let valor_total = valor_range * valor_unidade;
            $('#preco').html(valor_total.toLocaleString('pt-br',{style: 'currency', currency: 'BRL'}));
